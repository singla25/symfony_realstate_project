    <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="2024-tab-pane" role="tabpanel" aria-labelledby="2024-tab" tabindex="0">
            {% set projects = [
                { name: 'TruHaven Towers', location: 'Shimla', img: '1.jpg'},
                { name: 'TruHaven Elite Homes', location: 'New Chandigarh', img: '7.webp'},
                { name: 'TruHaven Shopping Square', location: 'Patiala Highway, Zirakpur', img: '10.webp'},
            ] %}
            <div class="owl-carousel delivered_project">
                {% for project in projects %}
                    <div class="item">
                        <div class="card h-100 border-0 shadow-sm">
                            <img src="{{ asset('photos/residentialProperty/' ~project.img) }}" alt="{{ project.name }}" class="card-img-top img-fluid" style="height: 200px; object-fit: cover;">
                            <div class="card-body text-center">
                                <h4><a href="#" class="text-decoration-none text-dark">{{ project.name }}</a></h4>
                                <div class="showLocation">
                                    <p>{{ project.location }}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>

